<%- model_class = PackageVersion -%>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:latest_version) %></th>
      <th><%= model_class.human_attribute_name(:title) %></th>
      <th><%= model_class.human_attribute_name(:published_at) %></th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "package_versions/compact",
     collection: @versions, as: :version %>
  </tbody>
</table>